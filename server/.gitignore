# .gitignore for a MERN stack project

# Node.js dependencies
node_modules/
npm-debug.log
yarn-error.log
package-lock.json
yarn.lock

# Environment variables
.env

# Logs
logs/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# OS generated files
.DS_Store
Thumbs.db

# IDE specific files
.vscode/
.idea/

# Build directories
dist/
build/
public/
coverage/

# Temporary files
tmp/
temp/
*.tmp

# Dependency directories
jspm_packages/
typings/

# Miscellaneous
*.sublime-project
*.sublime-workspace
*.log
*.swp
*.bak
*.pid
*.seed
*.pid.lock

# Static files
*.png
*.jpg
*.jpeg
*.gif
*.svg
*.ico

# API Keys and secrets
*.pem
*.cert
*.key
*.csr

# Serverless directories
.serverless/

# Optional npm cache directory
.npm/

# Optional eslint cache
.eslintcache

# Optional stylelint cache
.stylelintcache

# Optional REPL history
.node_repl_history

# Output of 'npm pack'
*.tgz

# Yarn Integrity file
.yarn-integrity

# dotenv environment variable file
.env.*

# Parcel-bundler cache (if used)
.cache/

# Svelte files
*.svelte

# Storybook files
.storybook/
